<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_graph"
    app:startDestination="@id/songsFragment">

    <fragment
    android:id="@+id/songsFragment"
    android:name="com.example.homework9.fragments.SongsFragment"
    android:label="SongsFragment"
    tools:layout="@layout/fragment_songs" >
    <action
        android:id="@+id/action_songsFragment_to_infoFragment"
        app:destination="@id/infoFragment"
        app:enterAnim="@anim/nav_default_enter_anim"
        app:exitAnim="@anim/nav_default_exit_anim"/>
    </fragment>

    <fragment
        android:id="@+id/infoFragment"
        android:name="com.example.homework9.fragments.InfoFragment"
        android:label="InfoFragment"
        tools:layout="@layout/fragment_information" >
        <action
            android:id="@+id/action_infoFragment_to_songsFragment"
            app:destination="@id/songsFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"/>
    </fragment>
</navigation>